# ========================================
# AQI Prediction System - Environment Variables
# ========================================
# Copy this file to .env and fill in your values

# API Keys
AQICN_API_KEY=your_aqicn_api_key_here
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Hopsworks Feature Store
HOPSWORKS_API_KEY=your_hopsworks_api_key_here
HOPSWORKS_PROJECT_NAME=aqi_prediction

# MLflow Tracking
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=aqi_prediction

# Prefect Settings
PREFECT_API_URL=http://localhost:4200/api

# Application Settings
APP_ENV=development
LOG_LEVEL=INFO
DEBUG=true

# API Settings
API_HOST=0.0.0.0
API_PORT=8000

# Dashboard Settings
DASHBOARD_PORT=8501

# Location Settings (default city for predictions)
DEFAULT_CITY=beijing
DEFAULT_LATITUDE=39.9042
DEFAULT_LONGITUDE=116.4074
